		# echo $i $1
		cat -s $i | sed "$PATTERN" | sed "s/\t//g" | sed "s/^ *//g" | sed "s/ *$//g" > ../web_$MODEL/$i
		../strip ../web_$MODEL/$i > tmpFile
		cp -rf tmpFile ../web_$MODEL/$i
	done

