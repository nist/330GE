# $Id: Makefile,v 1.1.1.1 2007/01/25 12:52:13 jiahao_jhou Exp $

CFLAGS = -Wall -g -O2
ifeq ($(strip $(DOSTATIC)),true)
    CFLAGS += --static
endif


all:		bidi tap

clean:
		rcsclean *
		rm -f *~
		rm -f bidi
		rm -f tap

bidi:		bidi.c
		$(CC) $(CFLAGS) -o bidi bidi.c

tap:		tap.c
		$(CC) $(CFLAGS) -o tap tap.c
